    "build-ui": "cd ui && yarn build",
    "predev": "yarn install",
    "dev": "nodemon --exec yarn build"
  },
  "devDependencies": {
    "eslint": "^8.30.0",
    "lerna": "^6.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1"
  }
}
