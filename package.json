{
    "name": "resources",
    "version": "1.0.0",
    "private": true,
    "description": "Codex - Fivem typescript boilerplate",
    "author": "Codex <mateus@codexfactory.tech",
    "scripts": {
        "install": "yarn install-ui && yarn install-resources",
        "install-ui": "cd ui && yarn",
        "install-resources": "cd resources && yarn",
        "build": "yarn lerna run build",
        "build-resources": "cd resources && yarn build",
        "build-ui": "cd ui && yarn build",
        "predev": "yarn install",
        "dev": "nodemon --exec yarn build"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.47.1",
        "@typescript-eslint/parser": "^5.47.1",
        "eslint": "^8.30.0",
        "lerna": "^6.1.0",
        "nodemon": "^2.0.20",
        "prettier": "^2.8.1",
        "typescript": "^4.9.4"
    }
}
